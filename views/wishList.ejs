 <!DOCTYPE html>
 <html lang="en">
 <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/styleSheets/wishList.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Imperial+Script&family=Pattaya&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
 </head>
 <body>
    <main>
        <div class="footer">
            <div class="footerItem home">
                <img src="/images/home (1).png" alt="">
                <p>Home</p>
            </div>
            <div class="footerItem profile">
                <img src="/images/user (1).png" alt="">
                <p>Profile</p>
            </div>
            <div class="footerItem cart">
                <img src="/images/shopping-cart.png" alt="">
                <p>Cart</p>
            </div>
            <div class="footerItem menu">
                <img src="/images/menu.png" alt="">
                <p>Menu</p>
            </div>
            <div class="footerItem support">
                <img src="/images/customer-service.png" alt="">
                <p>Support</p>
            </div>
        </div>

        <div class="header">
            <div class="back icon">
                <img src="/images/back.png" alt="">
            </div>
            <p>Wishlist & Collection</p>
            
            <div class="cartIcon icon">
                <img src="/images/shopping-cart.png" alt="">
            </div>
        </div>

        <div class="productCover">
            <%wishList.forEach(function(val){%>
                <div class="product"> <!--product start-->
                    <div class="productleft">
                        <div class="image">
                            <img src="<%=val.imageUrl%>" alt="">
                        </div>
                        
                    </div>
                    <div class="productRight">
                        <div class="name"><%=val.name%></div>
                        <div class="price">â‚¹<%=val.realPrice%></div>
                        <div class="available">In Stock</div>
                        <div class="option">
                            <div class="delete">Delete</div>
                            <div class="save">Add to cart</div>
                        </div>
                    </div>
                </div><!--product endl-->
           <% })%>
            </div>
           <% if(wishList.length!=0){%>
                <div class="proccedButton">
                    <p>Add To Cart</p>
                    <p class="totalItem">(<%=wishList.length%> items)</p>
                </div>
            <%}%>
            
            <div class="fake"></div>
        </div>
    </main>
    <script src="/javaScripts/nav.js"></script>

    <script src="/javaScripts/wishList.js"></script>
 </body>
 </html>